{
  "name": "hugo-titef",
  "version": "0.0.1",
  "description": "A Hugo theme. Developed for Titef documentation ",
  "main": "index.js",
  "scripts": {
    "build:sass": "sass --no-source-map ./static/css/index.scss ./static/css/_style.css",
    "build:autoprefix": "postcss ./static/css/_style.css --use autoprefixer -o ./static/css/style.css",
    "build": "npm run build:sass && npm run build:autoprefix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "postcss-cli": "^5.0.0",
    "sass": "^1.3.0"
  }
}
